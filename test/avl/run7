# Run 7: Wing and Tail with Finite Core Model
echo -e "oper\na a 5\nx\n\nquit\n" > commands
cat commands | avl wingtail2.avl > output
grep -o "CLtot.*" output
grep -o "CDtot.*" output
grep -o "CDff.*" output
grep -o "Cmtot.*" output
grep -o "CYtot.*" output
grep -o "Cltot.*" output
grep -o "Cntot.*" output
