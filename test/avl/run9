# Run 8: Simple Wing with Cosine-Spaced Spanwise and Chordwise Panels
echo -e "oper\na a 1\nx\n\nquit\n" > commands
cat commands | avl simplewing4.avl > output
grep -o "CLtot.*" output
grep -o "CDtot.*" output
grep -o "CDff.*" output
grep -o "Cmtot.*" output
grep -o "CYtot.*" output
grep -o "Cltot.*" output
grep -o "Cntot.*" output
